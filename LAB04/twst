#include <stdio.h>

int valor_max(int *peso, int *valor, int n, int m) {
    int soma_p, soma_v, v_max = 0;
    for (int i = 0; i < (1 << n); i++) {
        soma_p = 0;
        soma_v = 0;
        for (int j = 0; j < n; j++) { 
            if (i & (1 << j)) {
                soma_p += peso[j];
                soma_v += valor[j];
            }
        }
        if (soma_p <= m && soma_v > v_max) {
            v_max = soma_v;
        }
    }
    return v_max;
}

int main() {
    int n, m, v_otimo;
    scanf("%d %d", &n, &m);  
    int peso[n], valor[n];
    for (int i = 0; i < n; i++) {
        scanf("%d %d", &peso[i], &valor[i]); 
    }

    v_otimo = valor_max(peso, valor, n, m); 
    printf("%d\n", v_otimo);  
    
    return 0;
}